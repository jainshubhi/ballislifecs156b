users = 20;
movies = 5;
dummy = cell(users, 1);
for i = 1:users
    dummy{i} = zeros(movies, 2);
    for j = 1:movies
        dummy{i}(j, 1) = j;
        dummy{i}(j, 2) = randi([1, 5], 1);
    end
end
%%
F = 3;
% create W
W = rand(movies, F, 5);
%%
exp_data = zeros(movies, F, 5);
exp_recon = zeros(movies, F, 5);
for i = 1:users
    % populate V
    V = create_v(dummy{i});
    h = h_calc(V, W);
    exp_data = exp_calc(V, h);
    new_V = v_calc(h, W);
    
end